{
  "name": "geerydev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "webpack",
    "start": "NODE_ENV=production babel-node server.js",
    "start:dev": "NODE_ENV=dev nodemon server.js --exec babel-node --ext js,ejs,jsx",
    "heroku-postbuild": "NODE_ENV=production webpack",
    "test": "mocha --compilers js:babel-core/register --require ./js/test/setup.js 'js/test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch --watch-extensions jsx"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.24.1",
    "classnames": "^2.2.5",
    "connect-livereload": "*",
    "connect-mongo": "~0.8.2",
    "crypto": "0.0.3",
    "ejs": "^0.8.4",
    "ejs-layouts": "0.0.1",
    "express": "~3.4.3",
    "lodash": "^2.4.2",
    "mongoose": "~4.0.0",
    "mongoose-unique-validator": "~0.3.0",
    "newrelic": "*",
    "nodemailer": "*",
    "object.values": "^1.0.4",
    "passport": "latest",
    "passport-local": "latest",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^4.4.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.3.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "webpack": "^1.15.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cheerio": "^1.0.0-rc.2",
    "jsdom": "^8.0.4",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "~15.6.2",
    "react-hot-loader": "^1.3.0",
    "request": "^2.83.0",
    "sinon": "^2.3.7",
    "webpack-cli": "^2.1.4",
    "webpack-command": "^0.2.0",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": "^6.11.1"
  }
}
